{
  "name": "pixel-change",
  "version": "0.0.13",
  "description": "Measure differences between 2 identically sized buffer arrays of gray, rgb, or rgba pixels.",
  "main": "index.js",
  "scripts": {
    "install": "node-gyp rebuild",
    "pretest": "npm install",
    "test": "echo test",
    "preversion": "npm test",
    "__postversion": "npm run doc",
    "doc": "jsdoc index.js -d docs && git commit -m \"update docs\" -- docs",
    "local": "node test/local && node test/local2 && node test/local3",
    "compare": "node test/js_vs_cpp && node test/js_vs_cpp2 && node test/js_vs_cpp3",
    "regions": "node test/gray_regions && node test/rgb_regions && node test/rgba_regions",
    "prebuild": "prebuildify --napi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinGodell/pixel-change.git"
  },
  "keywords": [
    "pixel",
    "pixels",
    "change",
    "gray",
    "grey",
    "grayscale",
    "greyscale",
    "rgb",
    "rgba",
    "diff",
    "difference",
    "region",
    "regions",
    "motion",
    "detection"
  ],
  "author": "Kevin Godell <kevin.godell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinGodell/pixel-change/issues"
  },
  "homepage": "https://github.com/kevinGodell/pixel-change#readme",
  "private": false,
  "files": [
    "lib/",
    "src/",
    "binding.gyp"
  ],
  "gypfile": true,
  "engines": {
    "node": ">=8.5"
  },
  "dependencies": {
    "node-addon-api": "^1.2.0"
  },
  "devDependencies": {
    "jsdoc": "github:jsdoc3/jsdoc",
    "pipe2pam": "^0.6.2",
    "polygon-points": "^0.5.1",
    "prebuildify": "^2.11.0",
    "prebuildify-ci": "^1.0.4"
  }
}
